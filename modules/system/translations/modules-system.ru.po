msgstr "Переменный"

#: modules/system/system.admin.inc:1751
msgid "Value"
msgstr "Значение"

#: modules/system/system.admin.inc:1766
msgid "Command counters"
msgstr "Счетчики команд"

#: modules/system/system.admin.inc:1768
msgid "The number of <code>SELECT</code>-statements."
msgstr "Количество инструкций <code>SELECT</code>."

#: modules/system/system.admin.inc:1769
msgid "The number of <code>INSERT</code>-statements."
msgstr "Количество инструкций <code>INSERT</code>."

#: modules/system/system.admin.inc:1770
msgid "The number of <code>UPDATE</code>-statements."
msgstr "Количество инструкций <code>UPDATE</code>."

#: modules/system/system.admin.inc:1771
msgid "The number of <code>DELETE</code>-statements."
msgstr "Количество инструкций <code>DELETE</code>."

#: modules/system/system.admin.inc:1772
msgid "The number of table locks."
msgstr "Количество блокировок таблиц."

#: modules/system/system.admin.inc:1773
msgid "The number of table unlocks."
msgstr "Количество разблокировок таблиц."

#: modules/system/system.admin.inc:1776
msgid "Query performance"
msgstr "Производительность запросов"

#: modules/system/system.admin.inc:1778
msgid "The number of joins without an index; should be zero."
msgstr ""
"Количество присоединений, выполненных без использования индекса; должно быть "
"нулем."

#: modules/system/system.admin.inc:1780
msgid "The number of sorts done without using an index; should be zero."
msgstr ""
"Количество сортировок, выполненных без использования индекса; должно быть "
"нулем."

#: modules/system/system.admin.inc:1781
msgid "The number of times a lock could be acquired immediately."
msgstr "Количество раз, когда блокировка была нужна немедленно."

#: modules/system/system.admin.inc:1782
msgid "The number of times the server had to wait for a lock."
msgstr "Количество раз, когда серверу приходилось ожидать блокировку."

#: modules/system/system.admin.inc:1785
msgid "Query cache information"
msgstr "Информация о кеше запросов"

#: modules/system/system.admin.inc:1788
msgid "The number of queries in the query cache."
msgstr "Количество обращений в кеш запросов."

#: modules/system/system.admin.inc:377
msgid "Logo"
msgstr "Логотип"

#: modules/system/system.admin.inc:379
msgid "Site slogan"
msgstr "Слоган сайта"

#: modules/system/system.admin.inc:380
msgid "Mission statement"
msgstr "Миссия"

#: modules/system/system.admin.inc:381
msgid "User pictures in posts"
msgstr "Аватары в сообщениях"

#: modules/system/system.admin.inc:382
msgid "User pictures in comments"
msgstr "Аватары в комментариях"

#: modules/system/system.admin.inc:383
msgid "Search box"
msgstr "Строка поиска"

#: modules/system/system.admin.inc:384
msgid "Shortcut icon"
msgstr "Иконка"

#: modules/system/system.admin.inc:401
